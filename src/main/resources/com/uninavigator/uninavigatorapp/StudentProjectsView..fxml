<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.uninavigator.uninavigatorapp.StudentProjectsViewController">

    <VBox prefHeight="429.0" prefWidth="606.0">
        <Label text="Your Projects">
            <font>
                <Font size="24" />
            </font>
        </Label>
        <TableView fx:id="projectsTable">
            <columns>
                <TableColumn fx:id="projectNameColumn" prefWidth="150" text="Project Name" />
                <TableColumn fx:id="descriptionColumn" prefWidth="250" text="Description" />
                <TableColumn fx:id="instructorNameColumn" prefWidth="205.0" text="Instructor" />
            </columns>
        </TableView>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="151.0" text="Back" onAction="#handleBackButton" />
         </children>
      </HBox>
    </VBox>

</AnchorPane>
